{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug App",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "start:debug"],
      "restart": true,
      "console": "internalConsole",
      "outputCapture": "std",
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
